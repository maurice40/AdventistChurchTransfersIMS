@model ACTMS.Models.Division
@{
    ViewBag.Title = "New Division";
    Layout = "~/Views/Shared/_Common.cshtml";
}
<h2>Add New Church</h2>

@using (Html.BeginForm("Create", "Division"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Name)
        @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
    </div>
    
    <div class="form-group">
        @Html.LabelFor(m => m.Contacts)
        @Html.TextBoxFor(m => m.Contacts, new { @class = "form-control" })
    </div>
   <button class="btn btn-primary col-md-2" type="submit">Save</button>
}


